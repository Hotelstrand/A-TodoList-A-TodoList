<%- 
if_p('ha_proxy.cidrs_in_file') do |files| 
  files.each do |file|
    name = file["name"]
    content = ""
    file['cidrs'].each do |cidr|
      content << cidr.strip + "\n"  
    end
-%>
========================== 0600 /var/vcap/jobs/haproxy/config/cidrs/<%= name %>
# generated by cidrs.ttar.erb
<%= content %>
<%- 
  end
end -%>
